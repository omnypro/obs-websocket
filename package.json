{
  "name": "@omnypro/obs-websocket",
  "version": "5.6.2",
  "description": "Modern OBS WebSocket client for Bun runtime",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun run build.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "biome check --apply .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "obs",
    "websocket",
    "obs-websocket",
    "streaming",
    "bun",
    "typescript"
  ],
  "author": "Omnypro",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@types/bun": "latest",
    "bun-plugin-dts": "^0.3.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omnypro/obs-websocket.git"
  },
  "bugs": {
    "url": "https://github.com/omnypro/obs-websocket/issues"
  },
  "homepage": "https://github.com/omnypro/obs-websocket#readme"
}
